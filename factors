#!/usr/bin/python3

import sys

# Function to factor a number into two smaller numbers
def factorize_number(n):
    for i in range(2, n):
        if n % i == 0:
            return (i, n // i)
    return None

if len(sys.argv) != 2:
    print("Usage: factors <file>")
    sys.exit(1)

input_file = sys.argv[1]

try:
    with open(input_file, 'r') as file:
        for line in file:
            num = int(line.strip())
            factors = factorize_number(num)
            if factors:
                p, q = factors
                print(f"{num}={p}*{q}")
except FileNotFoundError:
    print(f"File not found: {input_file}")
    sys.exit(1)
except ValueError:
    print("Invalid input in the file. All lines should be valid natural numbers greater than 1.")
    sys.exit(1)

