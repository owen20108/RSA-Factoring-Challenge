#!/usr/bin/python3
import sys

def factorize(n):
    for p in range(2, n):
        if n % p == 0:
            q = n // p
            return f"{n}={p}*{q}"
    return f"{n}=1*{n}"

if len(sys.argv) != 2:
    print("Usage: factors <file>")
    sys.exit(1)

input_file = sys.argv[1]

try:
    with open(input_file, "r") as file:
        for line in file:
            n = int(line.strip())
            factorization = factorize(n)
            print(factorization)
except FileNotFoundError:
    print(f"File not found: {input_file}")
    sys.exit(1)
except ValueError:
    print("Invalid input. All lines in the file should be valid natural numbers.")
    sys.exit(1)
except Exception as e:
    print(f"An error occurred: {str(e)}")
    sys.exit(1)

